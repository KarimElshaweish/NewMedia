<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/photoReport"
    android:layout_margin="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <HorizontalScrollView
        android:id="@+id/horizontalView1"
        android:layout_gravity="end"
        android:layout_height="wrap_content"
        android:scrollbars="horizontal|vertical"
        android:layout_width="wrap_content"
        android:layout_marginTop="5dip">

        <LinearLayout
            android:background="@drawable/black_border"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:padding="7dp"
                        android:background="@drawable/black_border"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/date"
                        android:layout_gravity="center_horizontal"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/date"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:padding="7dp"
                        android:background="@drawable/black_border"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/mobile_number"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/mobile_number"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="match_parent"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:text="@string/place"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/place"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"
android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/money_get_way"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/moneyGetWay"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"

                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/items"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:padding="5dp"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/items"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/money_rest"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/rest"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:textColor="#000"

                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"
android:gravity="center"
                        android:textColor="#000"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/money_get"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/moneyGet"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"

                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="match_parent"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:text="@string/price"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/price"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/client_name"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView                         android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/fullName"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@drawable/black_border"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:orientation="vertical">
                    <TextView
                        android:background="@drawable/black_border"
                        android:padding="7dp"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/rank"
                        android:fontFamily="@font/merssi_semibold"/>
                    <TextView
                        android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:textColor="#000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/rank"
                        android:fontFamily="@font/merssi_semibold"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
</HorizontalScrollView>
</ScrollView>